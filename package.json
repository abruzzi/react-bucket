{
  "name": "react-bucket",
  "version": "1.0.0",
  "description": "Starter Kit for React Application",
  "main": "index.js",
  "ava": {
    "files": [
      "app/src/**/*.spec.js"
    ],
    "source": [
      "app/src/**/*.{js,jsx}"
    ],
    "require": [
      "babel-register",
      "ignore-styles",
      "./config/test-setup.js"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "test:watch": "npm test -- --watch",
    "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test ava",
    "prebuild": "rimraf dist/public/**",
    "prepush": "npm test && npm run lint",
    "build": "webpack --config webpack.config.prod.js  --progress --colors",
    "start": "webpack-dev-server --config webpack.config.js --progress --inline  --colors --port 1337 --host 0.0.0.0",
    "start:mock": "TARGET=http://localhost:8080 npm start",
    "lint": "./node_modules/.bin/eslint ./app/src --ext .js",
    "predeploy": "rimraf dist/public/**",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js"
  },
  "keywords": [
    "redux",
    "react",
    "webpack",
    "sass",
    "css modules"
  ],
  "engines": {
    "node": "8.6.0",
    "npm": "5.3.0"
  },
  "author": "ThoughtWorks",
  "dependencies": {
    "antd": "^2.13.9",
    "axios": "0.12.0",
    "bowser": "^1.8.1",
    "cookie-machine": "0.1.1",
    "core-js": "^2.5.1",
    "cross-env": "^3.1.4",
    "currency-formatter": "^1.3.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "extract-text-webpack-plugin": "v2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.10.0",
    "morgan": "^1.7.0",
    "node-env-file": "^0.1.8",
    "offline-plugin": "^4.6.1",
    "precss": "^1.4.0",
    "react": "^15.1.0",
    "react-addons-update": "^15.3.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-joyride": "^1.11.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-auth-wrapper": "^0.8.0",
    "redux-thunk": "^1.0.0",
    "serialize-javascript": "^1.3.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "ava": "^0.23.0",
    "axios-mock-adapter": "1.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.26.2",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "enzyme-to-json": "^1.0.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "file-loader": "^0.10.1",
    "history": "^4.5.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.7.1",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "markdown-loader": "^2.0.0",
    "minimist": "^1.2.0",
