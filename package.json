    "redux-devtools": "^3.0.1",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.3.0",
    "reselect": "^2.5.4",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.5.4",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^6.0.2",
    "sessionstorage": "0.0.2",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.6",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
