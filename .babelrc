{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "lodash", // Chain sequences arenâ€™t supported, workaround is https://medium.com/making-internets/why-using-chain-is-a-mistake-9bc1f80d51ba
    ["import", {"libraryName": "antd"}]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    },
    "production": {
      "plugins": [
        [
          "transform-decorators-legacy",
          "babel-plugin-webpack-alias",
          {
            "config": "./webpack.config.prod.js"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        [
          "transform-async-to-generator"
        ],
        [
          "transform-decorators-legacy"
        ],
        [
          "transform-es2015-modules-commonjs"
        ],
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./webpack.config.js"
          }
        ],
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ]
      ]
    }
  }
}